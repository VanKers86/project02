{# extend the parent tpl #}
{% extends 'layout.twig' %}

{% block pageHeader %}
<section class="center">
<header id="small" class="clearfix">
    <h1><a href="/">Dieetopvolging</a></h1>
    <ul id="menu">
        <li><a id="dashboard" href="/dietist/console" class="{% if dashboardActive is defined %}activeDashboard{% endif %} passiveDashboard">Dashboard</a></li>
        <li><a id="customers" href="/dietist/klanten" class="{% if customersActive is defined %}activeCustomers{% endif %} passiveCustomers">Klanten</a></li>
        <li><a href="#">Link 3</a></li>
    </ul>
</header>
{% endblock %}

{% block pageContent %}
<div id="consoleContent" class="clearfix">
    <a href="{{ app.request.baseUrl }}/dietist/logout">Afmelden</a>    
    {% block consolePageContent %}

    {% endblock %}
</div>
</section>
{% endblock %}